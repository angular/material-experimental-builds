    "./package.json": {
      "default": "./package.json"
    },
    "./column-resize": {
      "types": "./column-resize/index.d.ts",
      "default": "./fesm2022/column-resize.mjs"
    },
    "./menubar": {
      "types": "./menubar/index.d.ts",
      "default": "./fesm2022/menubar.mjs"
    },
    "./popover-edit": {
      "types": "./popover-edit/index.d.ts",
      "default": "./fesm2022/popover-edit.mjs"
    },
    "./selection": {
      "types": "./selection/index.d.ts",
      "default": "./fesm2022/selection.mjs"
    }
  },
  "peerDependencies": {
    "@angular/cdk": "21.0.0-next.2",
    "@angular/core": "^21.0.0-0 || ^21.1.0-0 || ^21.2.0-0 || ^21.3.0-0 || ^22.0.0-0",
    "@angular/common": "^21.0.0-0 || ^21.1.0-0 || ^21.2.0-0 || ^21.3.0-0 || ^22.0.0-0",
    "@angular/forms": "^21.0.0-0 || ^21.1.0-0 || ^21.2.0-0 || ^21.3.0-0 || ^22.0.0-0",
    "@angular/platform-browser": "^21.0.0-0 || ^21.1.0-0 || ^21.2.0-0 || ^21.3.0-0 || ^22.0.0-0",
    "@angular/material": "21.0.0-next.2",
    "@angular/cdk-experimental": "21.0.0-next.2"
  },
  "devDependencies": {
    "@angular/material": "workspace:*",
    "@angular/cdk": "workspace:*",
    "@angular/cdk-experimental": "workspace:*"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "ng-update": {
    "packageGroup": [
      "@angular/material",
      "@angular/cdk",
      "@angular/cdk-experimental",
      "@angular/material-experimental",
      "@angular/material-luxon-adapter",
      "@angular/material-moment-adapter",
      "@angular/material-date-fns-adapter"
    ]
  },
  "sideEffects": false,
  "module": "./fesm2022/material-experimental.mjs",
  "typings": "./index.d.ts",
  "type": "module"
